@model IEnumerable<ContosoModels.Models.Department>
@{
    ViewBag.Title = "Index";
}

<h2>Departments</h2>

<table>
    <tr>
        <td>Departments</td>
    </tr>
    @foreach (var department in Model)
    {
        <tr>
            <td>@Html.ActionLink(department.Name,"Detail",department)</td>
        </tr>
    }
    <tr>
        <td>@Html.ActionLink("add new department","Create")</td>
    </tr>
    
</table>

