@model  IEnumerable<ImageSharingWithCloudStorage.Models.ApplicationUser>

<h2>@ViewBag.Title</h2>

<table>

    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>@Html.ActionLink("Active", "doActive", new { Id = user.Id })</td>
        </tr>
    }
</table>
